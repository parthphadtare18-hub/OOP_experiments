Problem Statement 1
#include <iostream>
#include <stack>
using namespace std;

int main() {
    stack<int> s;
    s.push(10);
    s.push(20);
    s.push(30);
    s.pop();

    cout << "Top element: " << s.top();
    return 0;
}

Problem Statement 2
#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;
    q.push(1);
    q.push(2);
    q.push(3);
    q.pop();

    cout << "Front element: " << q.front();
    return 0;
}
